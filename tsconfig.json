{
   "compilerOptions": {
     "allowJs": false,
     "target": "ES6",
     "module": "commonjs",
     "removeComments": true,
     "noImplicitAny": true,
     "moduleResolution": "node",
     "allowUnreachableCode": false,
     "allowUnusedLabels": false,
     "noUnusedLocals": true,
     "noUnusedParameters": false,
     "preserveConstEnums": false,
     "importHelpers": true,
     "baseUrl": "./",
     "esModuleInterop": true,
     "lib": ["es5", "es6", "es2015"]
   }
 }
 